#include <iostream>
using namespace std;



int main() {
    int arr[]={1,2,3,4};
    int size = 4;
    int first=0;
    int last=size-1;
    while(first<last){
        swap(arr[first],arr[last]);
        last--;
        first++;
    }
    for(int i=0;i<size;i++){
        cout<<arr[i]<<endl;
    }

    return 0;
}
