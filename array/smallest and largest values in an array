// enter marks
// 12 13 14 15 1 
// largest number is 15
// smallest number is 1




#include <iostream>
using namespace std;

int main() {
   int size=5;
   int smallest,largest;
    int marks[size];
    cout<<"enter marks"<<endl;
    for(int i=0;i<size;i++){
        cin>>marks[i];
    }
    smallest=marks[0];
    largest=marks[0];
    for(int i=1;i<size;i++){
        if(marks[i]<smallest){
            smallest=marks[i];
        }
        if(marks[i]>largest){
            largest=marks[i];
        }
    }
    cout<<"largest number is "<<largest<<endl;
    cout<<"smallest number is "<<smallest<<endl;
    return 0;
}
