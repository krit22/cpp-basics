#include <iostream>
#include <vector>
using namespace std;

int main() {

    int x=2;
    int n=4;
    int product=1;
    int number=x;
    
    while(n>0){
        int temp=n%2;
        if(temp==1){
            product*=number;
        }
        n/=2;
        number*=number;
    }
    cout<<product;
}
