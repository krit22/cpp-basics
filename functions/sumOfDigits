// output:
// enter a number
// 45
// 9


using namespace std;
#include <iostream>

int sumOfDigits(int n){
    int sum=0;
    while(n>0){
        sum+=n%10;
        n/=10;
    }
    return sum;
}

int main() {
    cout<<"enter a number\n";
    int n;
    cin>>n;
    cout<<sumOfDigits(n)<<endl;
    return 0;
}


